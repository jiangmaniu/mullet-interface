{
  "name": "stellux-client",
  "version": "6.0.0",
  "private": true,
  "description": "stellux-client",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 max build",
    "build:prod": "cross-env APP_ENV=prod max build",
    "build:test": "cross-env APP_ENV=test max build",
    "cz": "git add . && git-cz",
    "dev": "npm run start:dev",
    "dev:prod": "npm run start:prod",
    "dev:test": "npm run start:test",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "max setup",
    "lint": "npm run lint:js && npm run lint:prettier && npm run tsc",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:css": "stylelint src/**/*.{css,scss,less} --fix --custom-syntax postcss-scss",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src ",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier -c --write \"**/**.{js,jsx,tsx,ts,less,md,json}\" --end-of-line auto",
    "prepare": "husky install",
    "prettier": "prettier -c --write \"**/**.{js,jsx,tsx,ts,less,md,json}\"",
    "preview": "max preview --port 8000",
    "start": "cross-env UMI_ENV=dev max dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none APP_ENV=dev UMI_ENV=dev max dev",
    "start:mock": "cross-env REACT_APP_ENV=dev MOCK=1 UMI_ENV=dev max dev",
    "start:no-mock": "cross-env MOCK=none UMI_ENV=dev max dev",
    "start:pre": "cross-env REACT_APP_ENV=pre UMI_ENV=dev max dev",
    "start:prod": "cross-env REACT_APP_ENV=dev MOCK=none APP_ENV=prod UMI_ENV=dev max dev",
    "start:test": "cross-env REACT_APP_ENV=dev MOCK=none APP_ENV=test UMI_ENV=dev max dev",
    "tsc": "npx tsc --noEmit"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
