  "scripts": {
    "analyze": "cross-env ANALYZE=1 ANALYZE_PORT=8888 max build",
    "build": "max build && npm run clearPlatformFiles",
    "build:mullet:dev": "cross-env PLATFORM_SEO=1 env-cmd -f .env-conf/mullet/.env.mullet.dev max build",
    "build:mullet:prod": "npm run build && cp -r ./docs/平台配置物料/mullet/** ./dist/platform",
    "build:mullet:seo": "cross-env PLATFORM_SEO=1 env-cmd -f .env-conf/mullet/.env.mullet.prod max build",
    "clearPlatformFiles": "node scripts/clearPlatform.js",
    "cz": "git add . && git-cz",
    "dev:mullet:dev": "REACT_APP_ENV=dev MOCK=none UMI_ENV=dev env-cmd -f .env-conf/mullet/.env.mullet.dev max dev",
    "dev:mullet:local": "REACT_APP_ENV=dev MOCK=none UMI_ENV=dev env-cmd -f .env-conf/mullet/.env.mullet.local max dev",
    "dev:mullet:prod": "REACT_APP_ENV=dev MOCK=none UMI_ENV=dev env-cmd -f .env-conf/mullet/.env.mullet.prod max dev",
    "dev:mullet:test": "REACT_APP_ENV=dev MOCK=none UMI_ENV=dev env-cmd -f .env-conf/mullet/.env.mullet.test max dev",
    "gen:api": "esno ./scripts/services/genApi/index.ts",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "max setup",
    "lint": "npm run lint:js && npm run lint:prettier && npm run tsc",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:css": "stylelint src/**/*.{css,scss,less} --fix --custom-syntax postcss-scss",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src ",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier -c --write \"**/**.{js,jsx,tsx,ts,less,md,json}\" --end-of-line auto",
    "prepare": "husky install",
    "prettier": "prettier -c --write \"**/**.{js,jsx,tsx,ts,less,md,json}\"",
    "preview": "max preview --port 8000",
    "start:mock": "cross-env REACT_APP_ENV=dev MOCK=1 UMI_ENV=dev max dev",
    "start:no-mock": "cross-env MOCK=none UMI_ENV=dev max dev",
    "start:pre": "cross-env REACT_APP_ENV=pre UMI_ENV=dev max dev",
    "tsc": "npx tsc --noEmit --skipLibCheck"
  },
